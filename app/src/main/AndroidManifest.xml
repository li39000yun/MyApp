<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lyq.myapp">

    <!-- 添加网络请求权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--android:icon="@mipmap/ic_launcher"-->
    <application
        android:allowBackup="true"
        android:icon="@drawable/xb"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".DemoActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".IndexMainActivity"/>
        <!-- 工作日志 -->
        <activity android:name=".jeecg.work.activity.WorkLogAddActivity"/>
        <activity android:name=".jeecg.work.activity.WorkLogViewActivity"/>
        <activity android:name=".jeecg.work.activity.WorkLogEditActivity"/>
        <activity android:name=".jeecg.work.activity.WorkLogListActivity"/>
        <!-- 个人账户 -->
        <activity android:name=".jeecg.ims.activity.ImsAccountListActivity"/>
        <activity android:name=".jeecg.ims.activity.ImsAccountViewActivity"/>
        <!--设置-->
        <activity android:name=".jeecg.frame.SettingActivity"/>
    </application>

</manifest>
